// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Collection {
  id              String          @id
  name            String
  contract        String
  description     String
  floorSale       Json
  floorSaleChange Json
  createdAt       DateTime        @default(now())
  sets            ColectionsSet[]
}

model ColectionsSet {
  id          String       @id
  collections Collection[]
  createdAt   DateTime     @default(now())
}

model User {
  id       String @id
  username String
  password String
}
